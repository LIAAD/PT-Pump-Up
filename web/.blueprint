models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string:100 nullable' }
    Link: { email: 'string nullable', website: 'url nullable', github_url: 'url nullable', hugging_face_url: 'url nullable', papers_with_code_url: 'url nullable', paper_url: 'url nullable' }
    NlpTask: { short_name: string, full_name: 'string nullable', description: 'longtext nullable', standard_format: string, papers_with_code_ids: json, relationships: { belongsToMany: 'Dataset, MachineLearningModel' } }
    Dataset: { short_name: string, full_name: 'string nullable', description: 'longtext nullable', year: integer, relationships: { belongsTo: 'Link, ResourceStats', belongsToMany: NlpTask, hasMany: Author } }
    MachineLearningModel: { short_name: string, full_name: 'string nullable', description: 'longtext nullable', year: integer, relationships: { belongsTo: 'Link, ResourceStats', belongsToMany: NlpTask, hasMany: 'Result, Author' } }
    Author: { name: string, institution: string, relationships: { belongsTo: Link, belongsToMany: 'Dataset, MachineLearningModel' } }
    ResourceStats: { preservation_rating: string, standard_format: boolean, off_the_shelf: boolean }
    Result: { metric: string, value: float, relationships: { belongsTo: 'MachineLearningModel, Dataset:train_dataset, Dataset:validation_dataset, Dataset:test_dataset' } }
